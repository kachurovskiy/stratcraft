{
  "name": "stratcraft",
  "version": "1.0.0",
  "description": "Create strategies trading the entire stock market, backtest and optimize on historical data, execute paper and live trades in Alpaca",
  "main": "dist/server/server.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "node scripts/copy-assets.js",
    "generate-icons": "node scripts/generate-icons.js",
    "kill-port": "node scripts/kill-port.js 3000",
    "start": "npm run build && node dist/server/server.js",
    "start:clean": "npm run build && node dist/server/server.js",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "trading",
    "backtesting",
    "strategies",
    "stocks"
  ],
  "author": "Maxim Kachurovskiy",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.15.6",
    "@types/ssh2": "^1.11.12",
    "jest": "^29.5.0",
    "puppeteer": "^24.31.0",
    "ts-jest": "^29.1.0"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.9",
    "axios": "^1.6.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-handlebars": "^8.0.5",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "resend": "^6.1.2",
    "ssh2": "^1.15.0",
    "tar": "^7.5.7",
    "typescript": "^5.0.0"
  },
  "overrides": {
    "js-yaml": "3.14.2",
    "qs": "6.14.1"
  }
}
